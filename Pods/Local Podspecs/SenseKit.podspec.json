{
  "name": "SenseKit",
  "version": "0.1.0",
  "summary": "Toolkit for building Sense apps",
  "homepage": "https://github.com/hello/SenseKit",
  "authors": {
    "Delisa Mason": "iskanamagus@gmail.com",
    "Jimmy Lu": "jimmy.m.lu@gmail.com"
  },
  "source": {
    "git": "https://github.com/hello/SenseKit.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes/SenseKit.h",
  "subspecs": [
    {
      "name": "Analytics",
      "source_files": "Pod/Classes/Analytics/*",
      "dependencies": {
        "CocoaLumberjack": [
          "~> 2.0.0"
        ]
      }
    },
    {
      "name": "API",
      "source_files": "Pod/Classes/API/*",
      "dependencies": {
        "FXKeychain": [
          "~> 1.5.1"
        ],
        "AFNetworking": [
          "~> 3.1.0"
        ],
        "NSJSONSerialization-NSNullRemoval": [
          "~> 1.0.0"
        ]
      }
    },
    {
      "name": "BLE",
      "source_files": "Pod/Classes/BLE/**/*.{h,m}",
      "dependencies": {
        "LGBluetooth": [

        ],
        "SHSProtoBuf": [

        ],
        "iOSDFULibrary": [

        ],
        "CocoaLumberjack": [
          "~> 2.0.0"
        ]
      }
    },
    {
      "name": "Model",
      "source_files": "Pod/Classes/Model/**/*.{h,m}",
      "dependencies": {
        "CGFloatType": [
          "~> 1.3.1"
        ]
      }
    },
    {
      "name": "Service",
      "source_files": "Pod/Classes/Service/*",
      "dependencies": {
        "CocoaLumberjack": [
          "~> 2.0.0"
        ]
      }
    }
  ]
}
